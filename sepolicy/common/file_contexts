# executeables
/(vendor|system/vendor)/bin/hw/vendor\.lineage\.touch@1\.0-service\.ks01lte             u:object_r:hal_lineage_touch_default_exec:s0

# data files
/data/bcmnfc(/.*)?                                      u:object_r:nfc_vendor_data_file:s0
/data/system/yas(.*)                                    u:object_r:hal_data_file:s0

# device nodes
/dev/ssp_sensorhub                                      u:object_r:sensors_device:s0

# efs files
/efs/hw_offset                                          u:object_r:sensors_efs_file:s0

# sysfs
/sys/devices(/.*)?/input/input[2-4]/enabled             u:object_r:sysfs_hal_pwr:s0
/sys/devices/virtual/sec/sec_touchkey/keypad_enable     u:object_r:sysfs_sec_touchkey:s0

# Sensor Library
/(vendor|system/vendor)/lib/libsensirion_ks01_eur.so    u:object_r:vendor_sensor_lib_file:s0
